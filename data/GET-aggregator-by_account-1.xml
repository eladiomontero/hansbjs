<aggregators>
	<aggregator name="asdasd main" type="filters">
		<filter value="/asd/" action="include"/>
		<filter value="/asd/*" action="include"/>
	</aggregator>
	<aggregator name="asdasd Direct Post" type="filters">
		<filter value="/asdasd/*" action="exclude"/>
		<filter value="/asdsadasd/" action="exclude"/>
	</aggregator>
</aggregators>